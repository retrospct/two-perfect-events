{
  "name": "two-perfect-events",
  "description": "Two Perfect Events & Weddings website code made with GatsbyJs, Netlify, and DatoCMS.",
  "version": "0.0.3",
  "private": true,
  "author": "Justin Lee <me@jlee.cool>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/retrospct/two-perfect-events"
  },
  "bugs": {
    "url": "https://github.com/retrospct/two-perfect-events/issues"
  },
  "scripts": {
    "develop": "gatsby develop",
    "dev": "gatsby develop",
    "format": "prettier --write src/**/*.{ts,js}*",
    "lint": "yarn lint:ts && yarn lint:js",
    "lint:ts": "eslint --ext ts,tsx .",
    "lint:js": "eslint --ext js,jsx .",
    "lint:watch": "esw -w . --color --ext ts,tsx,js,jsx",
    "lint:fix": "yarn lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "serve": "gatsby serve",
    "storybook": "start-storybook -p 6006",
    "clean": "gatsby clean",
    "build": "gatsby build",
    "build:storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern '!test/tests/lint/**' && yarn lint"
    }
  },
  "dependencies": {
    "@svgr/webpack": "^5.4.0",
    "dotenv": "^8.2.0",
    "feather-icons": "^4.28.0",
    "framer-motion": "^1.10.3",
    "gatsby": "^2.21.0",
    "gatsby-image": "^2.4.0",
    "gatsby-plugin-manifest": "^2.4.0",
    "gatsby-plugin-offline": "^3.2.0",
    "gatsby-plugin-react-axe": "^0.4.0",
    "gatsby-plugin-react-helmet": "^3.3.0",
    "gatsby-plugin-sharp": "^2.6.0",
    "gatsby-plugin-styled-components": "^3.3.2",
    "gatsby-plugin-svg-sprite": "^2.0.1",
    "gatsby-plugin-svgr": "^2.0.2",
    "gatsby-plugin-typescript": "^2.4.0",
    "gatsby-source-datocms": "^2.2.1",
    "gatsby-source-filesystem": "^2.3.0",
    "gatsby-transformer-remark": "^2.8.0",
    "gatsby-transformer-sharp": "^2.5.0",
    "modern-css-reset": "^1.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-masonry-component": "^6.2.1",
    "styled-components": "^5.1.0",
    "use-media": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.3",
    "@types/jest": "^25.2.1",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/react-helmet": "^5.0.15",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "babel-jest": "^25.4.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-preset-gatsby": "^0.4.0",
    "babel-preset-react-app": "^9.1.2",
    "eslint": "^6.8.0",
    "eslint-config-gojutin": "0.0.16",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "eslint-watch": "^6.0.1",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.4.0",
    "jest-styled-components": "^7.0.2",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "ts-jest": "^25.4.0",
    "tslint": "^6.1.2",
    "tslint-immutable": "^6.0.1",
    "tslint-react": "^5.0.0",
    "typescript": "^3.8.3"
  }
}

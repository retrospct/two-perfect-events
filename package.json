{
  "name": "two-perfect-events",
  "private": true,
  "description": "Two Perfect Events & Weddings website code made with GatsbyJs, Netlify, and DatoCMS.",
  "version": "0.1.0",
  "author": "Justin Lee <me@jlee.cool>",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "portfolio",
    "events",
    "weddings"
  ],
  "scripts": {
    "build": "echo SITE_RECAPTCHA_KEY=$SITE_RECAPTCHA_KEY >> .env.production && gatsby build",
    "clean": "gatsby clean",
    "format": "prettier src/**/*.{ts,tsx,js,jsx,json,css,svg}* --write",
    "start": "gatsby develop -o -H 0.0.0.0",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx,.ts,.tsx,.json,.css .",
    "lint:fix": "./node_modules/.bin/eslint --ext .js,.jsx,.ts,.tsx,.json,.css . --fix",
    "reset": "rm -rf .cache/ public/",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "optimize:imgs": "node ./src/utils/optimize-images.js"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "dotenv": "^8.2.0",
    "gatsby": "^2.30.1",
    "gatsby-alias-imports": "^1.0.6",
    "gatsby-image": "^2.9.0",
    "gatsby-plugin-canonical-urls": "^2.8.0",
    "gatsby-plugin-facebook-pixel": "^1.0.3",
    "gatsby-plugin-favicon": "^3.1.6",
    "gatsby-plugin-google-analytics": "^2.9.0",
    "gatsby-plugin-manifest": "^2.10.0",
    "gatsby-plugin-nprogress": "^2.8.0",
    "gatsby-plugin-offline": "^3.8.0",
    "gatsby-plugin-react-helmet": "^3.8.0",
    "gatsby-plugin-sharp": "^2.12.0",
    "gatsby-plugin-sitemap": "^2.10.0",
    "gatsby-plugin-styled-components": "^3.8.0",
    "gatsby-plugin-webfonts": "^1.1.4",
    "gatsby-source-datocms": "^2.2.5",
    "gatsby-source-filesystem": "^2.9.0",
    "gatsby-source-instagram": "^0.9.0",
    "gatsby-transformer-remark": "^2.14.0",
    "gatsby-transformer-sharp": "^2.10.0",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.6.2",
    "react": "^17.0.1",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-dom": "^17.0.1",
    "react-google-recaptcha": "^2.0.1",
    "react-helmet": "^6.0.0",
    "react-toggle": "^4.1.1",
    "styled-components": "^5.2.1",
    "use-media": "^1.4.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "babel-eslint": "^10.0.0",
    "babel-plugin-styled-components": "^1.12.0",
    "eslint": "^7.17.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-html": "^6.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.0.8",
    "minimist": "^1.2.5",
    "prettier": "^2.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/retrospct/two-perfect-events"
  },
  "bugs": {
    "url": "https://github.com/retrospct/two-perfect-events/issues"
  }
}
